# Copy this file to terraform.tfvars and configure your values

aws_region     = "us-west-2"
project_name   = "intervals-mapper"
intervals_api_key = "your-intervals-icu-api-key-here"

# Optional features
enable_function_url        = false
enable_scheduled_execution = true
schedule_expression       = "rate(6 hours)"  # Run every 6 hours